<form method="post" action="{{ path('app_admin_album_delete', {'id': album.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cet élement?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ album.id) }}">
    <button class="btn delete-button">Supprimer l'album</button>
</form>
