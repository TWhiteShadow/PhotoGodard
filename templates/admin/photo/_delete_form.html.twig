<form method="post" action="{{ path('app_admin_photo_delete', {'id': photo.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cet élement?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ photo.id) }}">
    <button class="btn delete-button">Supprimer</button>
</form>
